Host *
  ConnectTimeout 5
  AddKeysToAgent yes
  UseKeychain yes

Host wamba
  Hostname 172.16.128.15
  User andrea

Host raspberry
  Hostname 172.16.128.220
  User pi
  IdentityFile ~/.ssh/id_rsa_new
  IdentitiesOnly yes

Host rp4
  Hostname 192.168.84.4
  User pi
  IdentityFile ~/.ssh/id_rsa_new
  IdentitiesOnly yes

Host bb
  Compression yes
  Hostname bitbucket.org
  User hg

Host github
    Hostname github.com
    User ariciputi
    IdentityFile ~/.ssh/github_key
    IdentitiesOnly yes
